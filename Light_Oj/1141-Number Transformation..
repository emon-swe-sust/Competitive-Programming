#include<bits/stdc++.h>
using namespace std;
vector<int> mat[1001];
 
void primefactor( int n){
    int ind = n;
    int div = 2;
    int add = 0 ;
    while(n > 1){
        add = 0;
        while( n % div == 0 ){
            add = div;
            n /= div;
        }
        div += 1;
        if( add ) mat[ind].push_back(add);
    }
    if( add == ind ) mat[ind].clear();
}
 
int main()
{
    int test,m;
    cin >> test;
    for(m=0;m<1001;m++) primefactor(m);
    for(m=0; m<test; m++)
    {
        int a,b,minn[1001],i,j,k,l,q;
        memset(minn,-1,sizeof(minn));
        cin >> a >> b;
        queue<int>srl;
        srl.push(a);
        minn[a] = 0;
        while(!srl.empty())
        {
            k=srl.front();
            srl.pop();
            for(i=0 ; i<mat[k].size() ; i++)
            {
                l=k+mat[k][i];
                q=minn[k]+1;
                //cout << k << " " << mat[i] << " " << q << " " << minn[k] << endl;
                if(q < minn[l] || minn[l] == -1)
                {
                    minn[l] = q;
                    if(l < b)
                    {
                        srl.push(l);
                    }
                }
                if(l == b)
                    break;
            }
            if(l == b)
                break;
        }
        cout << "Case " << m+1 << ": " << minn[b] << endl;
    }
    return 0;
}
 
