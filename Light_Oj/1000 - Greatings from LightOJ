#include<stdio.h>
int main()
{
    int T,a,b,n;
    scanf("%d",&n);
    if(n<=125){
    int c[n];
    here:
 
        for(T=0;T<n;T++){
 
        scanf("%d",&a);
        if(0<=a&&a<=10)
        {
        scanf("%d",&b);
        if(0<=b&&b<=10)
        {
            c[T]=a+b;
        }
        else{
            continue;
    }
        }
    else{
        continue;
    }
        }
for(T=0;T<n;T++){
        printf("Case %d: %d\n",T+1,c[T]);
        }
 return 0;
}
}
 
