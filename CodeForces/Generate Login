#include<bits/stdc++.h>
using namespace std;

int main()
{
    string a,b,c="";
    int i=1,j=0,m,n;
    cin >> a >> b;
    c=c+a[0];
    while(a[i] < b[0] && i < a.size())
    {
        c=c+a[i];
        i++;
    }
    m=i-1;
   // cout << c << endl;
    c=c+b[0];
    cout << c << endl;
    return 0;
}
