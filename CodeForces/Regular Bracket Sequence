#include<bits/stdc++.h>
using namespace std;

int main()
{
    int c1,c2,c3,c4,left,right;
    cin >> c1 >> c2 >> c3 >> c4;
    left = c1*2+c2+c3;
    right = c2 + c3 + c4*2;
    if((c1==0 || c4 == 0) && c3 != 0)
    {
        cout << 0 << endl;
        return 0;
    }
    if(left == right)
        cout << 1 << endl;
    else
        cout << 0 << endl;

    return 0
