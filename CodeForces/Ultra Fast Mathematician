#include<stdio.h>
#include<string.h>

int main()
{
    char a[101],b[101];

    scanf("%s",a);
    scanf("%s",b);

    int i,j,k;

    i=strlen(a);

    char c[i];

    for(j=0;j<i;j++)
    {
        if(a[j]==b[j])
        {
            c[j]='0';
        }

        else
        {
            c[j]='1';
        }
    }

    c[i]='\0';

    printf("%s",c);

    return 0;
}
