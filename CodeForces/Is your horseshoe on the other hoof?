#include<stdio.h>

int main()
{
    int i,j,a0=0,a1=0,a2=0,a3=0,a[4],result;

    scanf("%d %d %d %d",&a[0],&a[1],&a[2],&a[3]);

    for(i=0;i<4;i++)
    {
        if(a[0]==a[i])
            a0++;
        if(a[1]==a[i])
            a1++;
        if(a[2]==a[i])
            a2++;
        if(a[3]==a[i])
            a3++;
    }


    if(a0==4)
        printf("3");

    else if(a0==3||a1==3||a0==2&&a1==2&&a2==2&&a3==2)
        printf("2");

    else if(a0==1&&a1==1&&a2==1)
        printf("0");

    else
        printf("1");


    return 0;
}
