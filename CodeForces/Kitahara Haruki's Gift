#include<bits/stdc++.h>
using namespace std;

int main()
{
    int now=0,i,n,j,n2=0,n1=0;
    cin >> n ;
    while(n--)
    {
        cin >> j;
        if(j == 200)
            n2++;
        else
            n1++;

    }
    if(n2 == 0)
    {
        if(n1 % 2==1)
        {
            cout << "NO" <<endl;
            return 0;
        }
        else
        {
            cout << "YES" << endl;
            return 0;
        }
    }
    if(n1 == 0)
    {
        if(n2%2 == 1)
        {
            cout << "NO" << endl;
            return 0;
        }
        else
        {
            cout << "YES" << endl;
            return 0;        }
    }
    if(n2 % 2 == 0)
    {
        if(n1 % 2 == 0)
        {
            cout << "YES" << endl;
            return 0;
        }
        else
        {
            cout << "NO" << endl;
            return 0;
        }
    }
    else
    {
        if(n1%2 == 1)
        {
            cout << "NO" << endl;
            return 0;
        }
        else
        {
            cout << "YES" << endl;
            return 0;
        }
    }
    return 0;
}
