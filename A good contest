#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,a,b;
    bool yes = false;
    cin >> n;
    string str;
    while(n--)
    {
        cin >> str >> a >> b;
        if(a >= 2400 && b > a)
            yes = true;
    }
    if(yes) cout << "YES" << endl;
    else cout << "NO" << endl;
    return 0;
}
